
<button>@Html.ActionLink("Register","Add","Employee",new { @class = "btn btn-primary"})</button>



<h2>Employees</h2>
<hr />
<table class="table table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Address</th>
        <th>Qualification</th>
        <th>DepartmentId</th>
        <th>SkillSet</th>
        <th>Manage Employee</th>
    </tr>
    @foreach (EmployeeData emp in Model)
    {
        <tr>
            <td>@emp.Employee_id</td>
            <td>@emp.Employee_Name</td>
            <td>@emp.Employee_Address</td>
            <td>@emp.Qualification</td>
            <td>@emp.Department_Id</td>
            <td>@emp.SkillsSet</td>
            <td>
                @Html.ActionLink("Update", "Edit", "Employee", new { id = @emp.Employee_id }, new { @class = "btn btn-primary" })
                @Html.ActionLink("Delete", "remove", "Employee", new { id = @emp.Employee_id },
            new { onclick = "return confirm('Do you want to delete?');", @class = "btn btn-warning" })
            </td>
        </tr>
       
    }
</table>